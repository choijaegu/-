package db.view;

import db.dao.EmpDAO;

public class MainApp 
{

	public static void main(String[] args) 
	{
		//1. 사원이름 검색

		System.out.println("**JDBC TEST ****");
		
		EmpDAO dao = new EmpDAO();
		
		dao.selectByEname();
		
		
		//2. 전체 사원정보 조회
		
		dao.selectAll();
		
		//3.특정 사원정보조회
		
		dao.selectByEmpno();
	}

}
